    "class Solution(object):\n",
    "    def findComplement(self, num):\n",
    "        \"\"\"\n",
    "        :type num: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        Bi=list(str(bin(num))[2:])\n",
    "        for i in range(len(Bi)):\n",
    "            Bi[i]=chr(ord('1')-ord(Bi[i])+ord('0'))\n",
    "        return int(\"\".join(Bi),2)"
   ]
  }
