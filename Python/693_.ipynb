    "class Solution:\n",
    "    def hasAlternatingBits(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        S=str(bin(n))[2:]\n",
    "        for i in range(1,len(S)):\n",
    "            if S[i]==S[i-1]:\n",
    "                return False\n",
    "        return True"
   ]
