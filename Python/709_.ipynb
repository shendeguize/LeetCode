   "source": [
    "class Solution:\n",
    "    def toLowerCase(self, str: str) -> str:\n",
    "        out = \"\"\n",
    "        for char in str:\n",
    "            if ord(\"A\") <= ord(char) <= ord(\"Z\"):\n",
    "                out += (chr(ord(\"a\") + ord(char) - ord(\"A\")))\n",
    "            else:\n",
    "                out += char\n",
    "        return out"
   ]
  }
