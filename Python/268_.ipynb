   "source": [
    "class Solution(object):\n",
    "    def missingNumber(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        n=len(nums)+1\n",
    "        D=dict(zip(range(n),[0]*n))\n",
    "        for item in nums:\n",
    "            D.pop(item)\n",
    "        return D.keys()[0]"
   ]
  }
