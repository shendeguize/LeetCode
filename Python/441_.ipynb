    "class Solution(object):\n",
    "    def arrangeCoins(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        i=1\n",
    "        while(1):\n",
    "            if i*(i+1)//2>n:\n",
    "                return i-1\n",
    "            else:\n",
    "                i+=1"
   ]
