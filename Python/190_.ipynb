    "class Solution:\n",
    "    # @param n, an integer\n",
    "    # @return an integer\n",
    "    def reverseBits(self, n):\n",
    "        Str_B=str(bin(n))[2:][::-1]\n",
    "        for i in range(32-len(Str_B)):\n",
    "            Str_B+='0'\n",
    "        return int(Str_B,2)"
   ]
